    "serializer"
  ],
  "author": "Lars Willighagen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kdl-org/kdljs/issues"
  },
  "homepage": "https://github.com/kdl-org/kdljs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdl-org/kdljs.git"
  },
  "dependencies": {
    "chevrotain": "^10.5.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^5.0.0",
    "jsdoc": "^4.0.2",
    "mocha": "^11.0.1",
    "standard": "^17.0.0"
  }
